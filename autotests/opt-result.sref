/* TREE */

MakeClosure {
  e.X = { = e.X; };
}

TestCopyClosure {
  s.X '>' '<' 's' =
    <s.X> <s.X>;
}

Go1 {
  =
    <TestCopyClosure
      <MakeClosure 'Hello'> '><s'
    >;
}

//------------------------------------------------------------------------------

MarkupArguments {
  s.Num ('--' e.Arg '=' e.Param) e.Tail = ;
}

Go2 {
  =
    <MarkupArguments 10 ('--hello=world') 'tail'>;
}

//------------------------------------------------------------------------------

Go3 {
  =
    <CopyE1 'abc!'><CopyE1 /* пусто */ '!'>;
}

NIL { e.Any = ; }

CopyE1 {
  e.Arg '!' = <NIL e.Arg e.Arg '!!'>;
}

//------------------------------------------------------------------------------

Go4 {
  =
    <CopyE2 ('abc') #X><CopyE2 (/* пусто */) #X>;
}

CopyE2 {
  (e.Arg) t.X = <NIL (e.Arg) '-' e.Arg t.X>;
}

//------------------------------------------------------------------------------

Go5 {
  =
    <{ '<Faaaa>' = ; }
      <{ t.X = '<F' t.X t.X t.X t.X '>'; } 'a'>
    >;
}

//------------------------------------------------------------------------------

$ENTRY Go {
    = <Go1> <Go2> <Go3> <Go4> <Go5>;
}
